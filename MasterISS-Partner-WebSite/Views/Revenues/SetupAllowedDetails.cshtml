@using MasterISS_Partner_WebSite.ViewModels.Revenues
@model  IEnumerable<GetBasicAllowenceDetailsResponseList>
@{
    ViewBag.Title = MasterISS_Partner_WebSite.Localization.View.SetupAllowedDetails;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <thead>
        <tr>
            <td>@Html.DisplayNameFor(m => m.AllowanceStateName)</td>
            <td>@Html.DisplayNameFor(m => m.Price)</td>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.AllowanceStateName)</td>
                <td>@Html.DisplayFor(modelItem => item.Price)</td>
            </tr>
        }
    </tbody>
</table>



